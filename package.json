{
  "name": "elflord",
  "version": "0.1.1",
  "author": "Mike Cantelon <mcantelon@gmail.com>",
  "description": "a standalone or client/server CLI utility for managing to-do items",
  "keywords": [
    "cli",
    "to-do",
    "todo",
    "productivity"
  ],
  "dependencies" : {
    "express"          : "2.5.x",
    "express-resource" : "0.2.x",
    "dirty"            : "0.9.x",
    "request"          : "2.2.x",
    "mingy"            : "0.2.x",
    "optimist"         : "0.3.x",
    "commander"        : "0.5.x"
  },
  "bin": {
    "elflord": "./bin/client.js",
    "elflord-server": "./bin/server.js"
  },
  "scripts": {
    "start": "./bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/mcantelon/node-elflord.git"
  },
  "bugs": {
    "url": "http://github.com/mcantelon/node-elflord/issues"
